<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Style x:Key="Background" TargetType="Window">
        <Setter Property="Background" Value="#37393d" />
    </Style>
    <Style x:Key="CustomDataGridStyle" TargetType="DataGrid">
        <Setter Property="Background" Value="#37393D" />
    </Style>
    <Style x:Key="label" TargetType="Label">
        <Setter Property="Foreground" Value="White"/>
    </Style>
    <Style x:Key="StockWhite" TargetType="CheckBox">
        <Setter Property="Foreground" Value="White"/>
    </Style>
    <Style x:Key="menu" TargetType="Menu">
        <Setter Property="Background" Value="#36393E" />
        <Setter Property="Foreground" Value="White"/>
    </Style>
    <Style x:Key="menuitem" TargetType="MenuItem">
        <Setter Property="Foreground" Value="#36393E" />
    </Style>
    <Style x:Key="backPage" TargetType="Page">
        <Setter Property="Background" Value="#37393D" />
    </Style>

</ResourceDictionary>